{
  "name": "Pluralize.name",
  "version": "0.5.0",
  "description": "Pluralize an English-language family name.",
  "author": "Trevor Farlow <npm@trevdor.com>",
  "bugs": {
    "url": "https://github.com/nematode/pluralize.name/issues"
  },
  "engines": {
    "node": "12.4.0"
  },
  "homepage": "https://github.com/nematode/pluralize.name#readme",
  "keywords": [
    "pluralize",
    "family name",
    "christmas cards"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevdor/pluralize.name.git"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest -w",
    "check": "yarn lint && yarn test",
    "build": "parcel build src/index.html"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "milligram": "1.3.0",
    "preact": "8.4.2",
    "preact-compat": "3.19.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "8.0.1",
    "babel-eslint": "10.x",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "parcel-bundler": "1.12.3",
    "prop-types": "15.7.2",
    "rimraf": "2.6.3",
    "sinon": "7.3.2"
  }
}
